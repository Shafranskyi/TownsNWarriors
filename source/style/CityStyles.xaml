			<Setter.Value>
				<ControlTemplate TargetType="Label">
					<Grid Background="{TemplateBinding Background}">
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>